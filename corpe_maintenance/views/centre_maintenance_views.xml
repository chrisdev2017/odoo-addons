<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="view_centre_maintenance_form" model="ir.ui.view">
            <field name="name">corpe_maintenance.centre_maintenance.form</field>
            <field name="model">corpe_maintenance.centre_maintenance</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="warehouse" colspan="1"/>
                            <field name="name" colspan="1"/>
                        </group>
                        <group></group>
                    </sheet>
                </form>


            </field>
        </record>

        <record id="view_centre_maintenance_tree" model="ir.ui.view">
            <field name="name">corpe_maintenance.centre_maintenance.tree</field>
            <field name="model">corpe_maintenance.centre_maintenance</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name" colspan="1"/>
                </tree>


            </field>
        </record>


         <record id="corpe_maintenance.centre_maintenance_action_view" model="ir.actions.act_window">
            <field name="name">Centre de maintenance</field>
            <field name="res_model">corpe_maintenance.centre_maintenance</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
    </data>
</openerp>